<launch>

  <arg name="e2e_control_node_param"  default="$(find-pkg-share e2e_control_node)/config/e2e_control_node.param.yaml"/>
  <!-- e2e_control_node -->
  <node pkg="e2e_control_node" 
        exec="e2e_control_node"
        name="e2e_control_node"
        output="screen">

    <!-- パラメータ -->
    <param from="$(var e2e_control_node_param)"/>
  </node>


</launch>